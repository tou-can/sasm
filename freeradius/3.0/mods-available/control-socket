control {
    #  Whether or not to create a control socket
    enabled = yes

    #  The location of the socket.
    #
    #  If you set this to anything other than ${run_dir}/freeradius.sock,
    #  then you MUST ALSO SET the "radiusd.conf" file:
    #
    #       security {
    #               user = freerad
    #               group = freerad
    #       }
    socket = ${run_dir}/freeradius.sock
}
